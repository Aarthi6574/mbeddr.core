<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="904d8797-3aec-486c-9cd8-d4d44b27c311" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/10.0/project-config.xsd">
  <name>Build org.iets3.core</name>
  <description />
  <settings>
    <options>
      <option name="artifactRules" value="artifacts/org.iets3.core =&gt; org.iets3.core&#xA;artifacts/org.iets3.analysis.z3 =&gt; org.iets3.analysis.z3&#xA;artifacts/org.iets3.distro =&gt; org.iets3.distro&#xA;artifacts/org.iets3.build =&gt; org.iets3.build" />
      <option name="checkoutMode" value="ON_AGENT" />
      <option name="cleanBuild" value="true" />
    </options>
    <disabled-settings>
      <setting-ref ref="vcsTrigger" />
    </disabled-settings>
    <parameters>
      <param name="iets3-artifacts-root.path-properties" value="-Diets3.artifacts.z3Lib=%teamcity.build.checkoutDir%/artifacts/org.iets3.analysis.z3 -Diets3.artifacts.core=%teamcity.build.checkoutDir%/artifacts/org.iets3.core" />
      <param name="iets3.checkout-rules" value="+:.=&gt;iets3.core" />
      <param name="iets3.home-path-proprties" value="-Diets3.home=%teamcity.build.checkoutDir%/iets3.core/code/languages/org.iets3.core -Diets3.github.core.home=%teamcity.build.checkoutDir%/iets3.core" />
      <param name="mbeddr.checkout-rules" value="+:.=&gt;mbeddr.core" />
      <param name="mbeddr.path-properties" value="-Dmbeddr.github.core.home=%teamcity.build.checkoutDir%/mbeddr.core -Dsl-all=%teamcity.build.checkoutDir%/mbeddr.core/code/plugins/sl-all" />
      <param name="mps.artifact-rule" value="MPS-mbeddr=&gt;%teamcity.build.checkoutDir%/MPS-mbeddr" />
      <param name="mps.path-properties" value="-Dmps_home=%teamcity.build.checkoutDir%/MPS-mbeddr -Dmps.home=%teamcity.build.checkoutDir%/MPS-mbeddr" />
    </parameters>
    <build-runners>
      <runner id="RUNNER_8" name="Build" type="Ant">
        <parameters>
          <param name="build-file-path" value="iets3.core/code/languages/build.xml" />
          <param name="runnerArgs" value="%mps.path-properties% %mbeddr.path-properties% %iets3.home-path-proprties% %build-dir.path-properties% %artifacts-root.path-properties% %artifacts-root.path-properties% %iets3-artifacts-root.path-properties%" />
          <param name="target" value="clean-and-build" />
          <param name="teamcity.coverage.emma.include.source" value="true" />
          <param name="teamcity.coverage.emma.instr.parameters" value="-ix -*Test*" />
          <param name="teamcity.coverage.idea.includePatterns" value="*" />
          <param name="teamcity.coverage.jacoco.patterns" value="+:*" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="Voluntis_Iets3">
        <checkout-rule rule="%iets3.checkout-rules%" />
      </vcs-entry-ref>
      <vcs-entry-ref root-id="Voluntis_Mbeddr">
        <checkout-rule rule="%mbeddr.checkout-rules%" />
      </vcs-entry-ref>
    </vcs-settings>
    <requirements />
    <build-triggers>
      <build-trigger id="vcsTrigger" type="vcsTrigger">
        <parameters>
          <param name="branchFilter" value="+:*" />
          <param name="quietPeriodMode" value="DO_NOT_USE" />
        </parameters>
      </build-trigger>
    </build-triggers>
    <artifact-dependencies>
      <dependency id="ARTIFACT_DEPENDENCY_86" sourceBuildTypeId="Voluntis_DownloadMps" cleanDestination="true">
        <revisionRule name="sameChainOrLastFinished" revision="latest.sameChainOrLastFinished" />
        <artifact sourcePath="%mps.artifact-rule%" />
      </dependency>
      <dependency id="ARTIFACT_DEPENDENCY_87" sourceBuildTypeId="Voluntis_BuildSlAllAndMbeddrPlatform" cleanDestination="true">
        <revisionRule name="sameChainOrLastFinished" revision="latest.sameChainOrLastFinished" />
        <artifact sourcePath="%generic.artifact-rule%" />
      </dependency>
    </artifact-dependencies>
    <dependencies>
      <depend-on sourceBuildTypeId="Voluntis_DownloadMps">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="Voluntis_BuildSlAllAndMbeddrPlatform">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

