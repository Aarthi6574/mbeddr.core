<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="117db1b2-c257-4825-a823-1e111487e90f" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/9.0/project-config.xsd">
  <name>Generate Build Scripts</name>
  <description />
  <settings>
    <options>
      <option name="checkoutDirectory" value="%teamcity.build.checkoutDir%/mbeddr.core" />
      <option name="cleanBuild" value="true" />
    </options>
    <parameters />
    <build-runners>
      <runner id="RUNNER_73" name="Generate Build Scripts" type="Ant">
        <parameters>
          <param name="ant.home" value="-Dmps.home=MPS -Dmps_home=MPS  -Dmbeddr.github.core.home=%teamcity.build.checkoutDir%" />
          <param name="build-file-path" value="code/languages/build.xml" />
          <param name="target" value="build-allScripts" />
          <param name="teamcity.coverage.emma.include.source" value="true" />
          <param name="teamcity.coverage.emma.instr.parameters" value="-ix -*Test*" />
          <param name="teamcity.coverage.idea.includePatterns" value="*" />
          <param name="teamcity.coverage.jacoco.patterns" value="+:*" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="Mbeddr2_Bootstrap_MbeddrBoostrap">
        <checkout-rule rule="+.=&gt;mbeddr.core" />
      </vcs-entry-ref>
    </vcs-settings>
    <requirements />
    <build-triggers />
    <artifact-dependencies>
      <dependency sourceBuildTypeId="Mbeddr2_Bootstrap_DownloadMps" cleanDestination="true">
        <revisionRule name="sameChainOrLastFinished" revision="latest.sameChainOrLastFinished" />
        <artifact sourcePath="%mps.artifact-paths%" />
      </dependency>
    </artifact-dependencies>
    <dependencies>
      <depend-on sourceBuildTypeId="Mbeddr2_Bootstrap_DownloadMps">
        <options>
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

