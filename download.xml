<project name="com.mbeddr.core download mps" default="download">
 
  <property name="MPS-Base-URL" value="http://download.jetbrains.com/mps/"/>
  <property name="MPS-File" value="MPS-117.9341.zip"/>
  <property name="MPS-Dir" value="MPS"/>  
